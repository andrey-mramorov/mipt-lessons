   "outputs": [],
   "source": [
    "def fibb(n):\n",
    "    _prev, _current = 0, 1\n",
    "    for _ in range(n):\n",
    "        yield _current\n",
    "        _next = _prev + _current\n",
    "        _prev = _current\n",
    "        _current = _next"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 9,
